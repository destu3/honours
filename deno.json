    "@supabase/supabase-js": "jsr:@supabase/supabase-js@^2.48.1",
    "cors": "npm:cors@^2.8.5",
    "drizzle-kit": "npm:drizzle-kit@^0.30.4",
    "drizzle-orm": "npm:drizzle-orm@^0.39.3",
    "express": "npm:express@^4.21.2",
    "express-winston": "npm:express-winston@^4.2.0",
